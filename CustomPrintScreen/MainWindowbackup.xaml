<Window x:Class="HookSample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Visibility="Hidden"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen"
        SizeToContent="Width"
        Title="MainWindow" Height="235" Width="635.5" Background="#3FFFFFFF">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="603*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel x:Name="imgs" Orientation="Horizontal" Margin="0,8,8,7" Grid.ColumnSpan="2"/>
        <Button Content="Zapisz" Margin="0,10,10,10" Grid.Row="1" HorizontalAlignment="Right" Width="78" Grid.Column="1" Click="Button_Click_1"/>
        <Button Content="Anuluj" Margin="0,10,93,10" Grid.Row="1" HorizontalAlignment="Right" Width="78" Grid.Column="1" Click="Button_Click"/>
        <TextBox x:Name="Filename" Height="31" Margin="0,0,179,0" Grid.Row="1" TextWrapping="Wrap" Text="TextBox" VerticalContentAlignment="Center" FontSize="14" HorizontalAlignment="Right" Width="200"/>
    </Grid>
</Window>
