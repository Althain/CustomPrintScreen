<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="CustomPrintScreen.MainWindow"
		Visibility="Hidden"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen"
        SizeToContent="WidthAndHeight"
        ResizeMode="NoResize"
        Closing="Window_Closing"
        Title="MainWindow" Height="0" Width="0" Background="#3FFFFFFF">
    <Grid Margin="0,8,8,8">
        <StackPanel x:Name="imgs" Margin="0" Orientation="Horizontal">
            <Grid Width="257" Visibility="Collapsed">
                <StackPanel Margin="0" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                    <Button Content="Button"  Margin="0" Width="60" Height="60" HorizontalContentAlignment="Center"/>
                    <Button Content="Button" Margin="10,0,0,0" Width="60" Height="60"/>
                </StackPanel>
            </Grid>
        </StackPanel>
    </Grid>
</Window>
